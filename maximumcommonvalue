#include <iostream>
using namespace std;

int main()
{
    int n, m;
    cin >> n >> m;

    
    long long a[n][m];

    
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            cin >> a[i][j];
        }
    }

  
    long long row_min[n];
    long long col_min[m];

    
    for (int i = 0; i < n; i++)
    {
        row_min[i] = a[i][0];
        for (int j = 1; j < m; j++)
        {
            if (a[i][j] < row_min[i])
                row_min[i] = a[i][j];
        }
    }

  
    for (int j = 0; j < m; j++)
    {
        col_min[j] = a[0][j];
        for (int i = 1; i < n; i++)
        {
            if (a[i][j] < col_min[j])
                col_min[j] = a[i][j];
        }
    }

    long long answer = 0;

    
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            if (row_min[i] == col_min[j])
            {
                if (row_min[i] > answer)
                    answer = row_min[i];
            }
        }
    }

    cout << answer << endl;

    return 0;
}
